@import '../../style/common.wxss';
@import '../../style/iconfont.wxss';
.login {
    text-align: center;
    position: relative;
}
.logo {
    margin: 0 auto;
    margin-top: 500rpx;
    width: 180rpx;
    height: 80rpx;
}
.logo image {
    display: inline-block;
    width: 100%;
    height: 100%;
}
.accreditBtn {
    display: flex;
