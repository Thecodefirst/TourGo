text, view {
    line-height: 1;
}
view {
    box-sizing: border-box;
}
.taro-img {
    display: inline-block;
    width: 100%;
    height: 100%;
}
button::after{
    border:none;
}
.taro-text {
    user-select: none;
}
.price {
    display: inline-block;
    font-size: 26rpx;
    font-family: din;
    font-weight: 500;
    color: #ba241b;
}
.li-elip {
    display: inline-block;
    font-size: 30rpx;
    width: 100%;
    overflow: hidden;
}
.li-elip-text {
    word-wrap: break-word;
    word-break: break-all;
    white-space: normal;
    text-overflow: ellipsis;
    overflow: hidden;
    line-height: 1.3;
    letter-spacing: 0;
    vertical-align: middle;
}
.li-title {
    box-sizing: border-box;
    font-size: 26rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-word;
    color: #232326;
    line-height: 1.4;
}